  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Sagar Desai",
      "email": "sagardesai1990@gmail.com"
    }
  ],
  "repositories": [
    {"type": "git", "url": "https://github.com/desaisagar/h5p-php-library.git"}
  ],
  "require": {
    "ext-zip": "*",
    "h5p/h5p-core": "dev-fix-php8-compatibility",
